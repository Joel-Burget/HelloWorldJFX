            <Button fx:id="deleteAppointment" layoutX="726.0" layoutY="327.0" mnemonicParsing="false" onAction="#deleteAppointmentAction" text="Delete" />
      <Label fx:id="customerWarningLabel" layoutX="94.0" layoutY="29.0" text="Label" textFill="#e80a0a" />
      <Label fx:id="appointmentWarningLabel" layoutX="567.0" layoutY="14.0" text="Label" textFill="#dd0707" />
